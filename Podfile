  pod 'CircleImageView'
  pod 'SDWebImage'

  target 'ModuleExamplesTests' do
    # Pods for testing
    use_frameworks!
    pod 'iOSSnapshotTestCase'
  end

  target 'ModuleExamplesUITests' do
    # Pods for testing
  end

end
